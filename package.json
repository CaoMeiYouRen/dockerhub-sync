{
  "name": "dockerhub-sync",
  "version": "0.1.0",
  "description": "同步 dockerhub 的镜像到其他镜像源",
  "author": "CaoMeiYouRen",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint src --fix --ext .ts,.js,.cjs,.mjs",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production tsup",
    "dev": "cross-env NODE_ENV=development tsx watch ./src/index.ts",
    "dev:tsup": "cross-env NODE_ENV=development tsup --watch",
    "start": "node ./dist/index",
    "start:tsx": "cross-env NODE_ENV=production tsx ./src/index.ts",
    "release": "semantic-release",
    "commit": "cz",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/debug": "^4.1.12",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^5.0.0",
    "conventional-changelog-cmyr-config": "^2.1.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-conventional-changelog-cmyr": "^1.1.0",
    "debug": "^4.3.5",
    "eslint": "^8.57.0",
    "eslint-config-cmyr": "^1.1.31",
    "eslint-plugin-import": "^2.28.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "rimraf": "^5.0.7",
    "semantic-release": "^23.0.6",
    "tsup": "^8.1.0",
    "tsx": "^4.15.1",
    "typescript": "^5.4.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog-cmyr"
    }
  },
  "changelog": {
    "language": "zh"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint",
      "git add"
    ]
  },
  "keywords": [
    "tsup",
    "typescript",
    "template"
  ],
  "homepage": "https://github.com/CaoMeiYouRen/dockerhub-sync#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaoMeiYouRen/dockerhub-sync.git"
  },
  "bugs": {
    "url": "https://github.com/CaoMeiYouRen/dockerhub-sync/issues"
  },
  "dependencies": {
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "fs-extra": "^11.2.0",
    "isomorphic-unfetch": "^4.0.2",
    "lodash-es": "^4.17.21",
    "log4js": "^6.9.1",
    "rss-parser": "^3.13.0",
    "tslib": "^2.6.3",
    "yaml": "^2.4.5",
    "zx": "^8.1.2"
  },
  "pnpm": {
    "overrides": {
      "compare-func": "^2.0.0"
    }
  }
}